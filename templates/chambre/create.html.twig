{% extends 'backBase.html.twig' %}

{% block body %}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="page-header">
            <h3 class="page-title"> chambres </h3>

        </div>
        <div class="col-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    {{ form_start(f, {'attr':{'novalidate':'novalidate'}}) }}
                    <div class="form-group">
                        <label for="productName">Type</label>
                        {{ form_widget(f.type,{'attr':{'class':'form-control'}}) }}


                    </div>

                    <div class="form-group">
                        <label for="productName">Vue</label>
                        {{ form_widget(f.vue,{'attr':{'class':'form-control'}}) }}

                    </div>

                    <div class="form-group">
                        <label for="productName">Prix</label>
                        {{ form_widget(f.prixNuitee,{'attr':{'class':'form-control'}}) }}

                    </div>
                    <div class="text-danger">{{ form_errors(f.prixNuitee) }}</div>

                    <div class="form-group">
                        <label for="productName">Hotel</label>
                        {{ form_widget(f.hotel,{'attr':{'class':'form-control'}}) }}


                    </div>

                    <button type="submit" class="btn btn-primary mr-2">Submit</button>

                    {{form_end(f)}}
                </div>
            </div>
        </div>
    </div></div>
        {% endblock %}
